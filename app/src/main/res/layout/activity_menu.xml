<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="in4matics_team.in4maticsquiz.menuActivity">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="@android:dimen/thumbnail_height"
        android:id="@+id/imageView3"
        android:src="@drawable/logo"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="199dp"
        android:layout_height="wrap_content"
        android:text="PROVIJERI ZNANJE"
        android:id="@+id/btnProvjeri"
        android:layout_gravity="center_horizontal"
        android:layout_weight="0.15"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/imageView3" />

    <Button
        android:layout_width="199dp"
        android:layout_height="wrap_content"
        android:text="RANG LISTE"
        android:id="@+id/btnLista"
        android:layout_gravity="center_horizontal"
        android:layout_weight="0.15"
        android:layout_below="@+id/btnProvjeri"
        android:layout_alignRight="@+id/btnProvjeri"
        android:layout_alignEnd="@+id/btnProvjeri"
        android:layout_alignLeft="@+id/btnProvjeri"
        android:layout_alignStart="@+id/btnProvjeri" />
</RelativeLayout>
